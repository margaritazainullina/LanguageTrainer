      <item name="android:indeterminateDrawable">@drawable/lime_scrubber_progress_horizontal_holo_dark</item>
      <item name="android:thumb">@drawable/lime_scrubber_control_selector_holo_dark</item>
  </style>

  <style name="RatingBarLime" parent="android:Widget.Holo.RatingBar">
      <item name="android:progressDrawable">@drawable/lime_ratingbar_full_holo_dark</item>
